{
    "compilerOptions": {
        "lib": [
            "es5",
            "es6"
        ],
         "target": "es6",
         "module": "commonjs",
         "moduleResolution": "node",
         "outDir": "./dist",
         "emitDecoratorMetadata": true,
         "experimentalDecorators": true,
         "sourceMap": true,
         "typeRoots": ["node_modules/@types", "node_modules/web3-typescript-typings"]      
    },
    "exclude": [
        "node_modules"
    ]
}
